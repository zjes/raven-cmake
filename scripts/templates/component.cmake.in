#Component: @comp@

#set(CPACK_DEBIAN_PACKAGE_DEBUG ON)
set(CPACK_GENERATOR "DEB")
set(CPACK_CMAKE_GENERATOR "Ninja")
set(DEFAULT @default@)
set(CPACK_DEB_COMPONENT_INSTALL @CPACK_DEB_COMPONENT_INSTALL@)
set(GEN_CPACK_DEBIAN_FAKEROOT_EXECUTABLE /usr/bin/fakeroot)

if(DEFAULT)
    set(CPACK_SOURCE_OUTPUT_CONFIG_FILE "@CMAKE_CURRENT_BINARY_DIR@/CPackSourceConfig.cmake")
    set(CPACK_OUTPUT_CONFIG_FILE "@CMAKE_CURRENT_BINARY_DIR@/CPackConfig.cmake")
    if (EXISTS ${CPACK_OUTPUT_CONFIG_FILE})
        include(${CPACK_OUTPUT_CONFIG_FILE})
    endif()
else()
    set(CPACK_SOURCE_OUTPUT_CONFIG_FILE "@CMAKE_CURRENT_BINARY_DIR@/CPackSourceConfig.@comp@.cmake")
    set(CPACK_OUTPUT_CONFIG_FILE "@CMAKE_CURRENT_BINARY_DIR@/CPackConfig.@comp@.cmake")
    if(CPACK_DEB_COMPONENT_INSTALL)
        set(CPACK_INSTALL_CMAKE_PROJECTS "@CMAKE_CURRENT_BINARY_DIR@;@PROJECT_NAME@;@CPACK_DEB_COMPONENT_INSTALL@;/")
    else()
        set(CPACK_INSTALL_CMAKE_PROJECTS "@CMAKE_CURRENT_BINARY_DIR@;@PROJECT_NAME@;ALL;/")
    endif()
endif()

if(NOT "@LD_LIBRARY_PATH@" STREQUAL "")
    set(ENV{LD_LIBRARY_PATH} "$ENV{LD_LIBRARY_PATH}:/usr/lib/x86_64-linux-gnu/:@LD_LIBRARY_PATH@")
endif()

set(CPACK_PACKAGE_NAME "@pkgname@")
set(CPACK_PACKAGE_DESCRIPTION_SUMMARY "@CPACK_PACKAGE_DESCRIPTION_SUMMARY@")
set(CPACK_PACKAGE_DESCRIPTION "@CPACK_PACKAGE_DESCRIPTION@")
set(CPACK_PACKAGE_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(CPACK_PACKAGE_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(CPACK_PACKAGE_VERSION_PATCH @PROJECT_VERSION_PATCH@)
set(CPACK_PACKAGE_VERSION @FULL_PROJECT_VERSION@)
set(CPACK_PACKAGE_CONTACT "@CPACK_PACKAGE_CONTACT@")
if("@shlibdeps@" STREQUAL "TRUE")
    set(CPACK_DEBIAN_PACKAGE_SHLIBDEPS ON)
    #set(CPACK_DEBIAN_PACKAGE_GENERATE_SHLIBS ON)
endif()

if(NOT "@CPACK_PACKAGE_DESCRIPTION_FILE@" STREQUAL "")
    set(CPACK_PACKAGE_DESCRIPTION_FILE "@CPACK_PACKAGE_DESCRIPTION_FILE@")
endif()

set(CPACK_STRIP_FILES @STRIP_FILES@)
set(CPACK_DEBIAN_PACKAGE_DEPENDS "@CPACK_DEBIAN_PACKAGE_DEPENDS@")
set(CPACK_DEBIAN_PACKAGE_REPLACES "@CPACK_DEBIAN_PACKAGE_REPLACES@")
set(CPACK_DEBIAN_PACKAGE_PROVIDES "@CPACK_DEBIAN_PACKAGE_PROVIDES@")
set(CPACK_DEBIAN_PACKAGE_BREAKS "@CPACK_DEBIAN_PACKAGE_BREAKS@")
set(CPACK_DEBIAN_PACKAGE_CONFLICTS "@CPACK_DEBIAN_PACKAGE_CONFLICTS@")
set(CPACK_DEBIAN_PACKAGE_CONTROL_EXTRA "@CPACK_DEBIAN_PACKAGE_CONTROL_EXTRA@")

set(LICENSE "@CPACK_RESOURCE_FILE_LICENSE@")
if (LICENSE)
    set(CPACK_RESOURCE_FILE_LICENSE ${LICENSE})
endif()

set(CPACK_PACKAGE_FILE_NAME "@CPACK_PACKAGE_FILE_NAME@")

if(CPACK_DEB_COMPONENT_INSTALL)
    set(CPACK_DEB_COMPONENT_INSTALL ON)
    set(CPACK_COMPONENTS_GROUPING "ALL_COMPONENTS_IN_ONE")
    set(CPACK_COMPONENTS_ALL "@comp@")
endif()

