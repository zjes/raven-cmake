@PACKAGE_INIT@

if (NOT RAVEN_CMAKE_CMAKE_DIR)
    if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/CMakeFiles")
        set(RAVEN_CMAKE_CMAKE_DIR "@CMAKE_CURRENT_SOURCE_DIR@")
    else()
        set(RAVEN_CMAKE_CMAKE_DIR "@PACKAGE_CMAKE_INSTALL_DIR@")
    endif()

    include(${RAVEN_CMAKE_CMAKE_DIR}/cmake/raven-cmake.cmake)
endif()

if (NOT RAVEN_CMAKE_CMAKE_FOUND)
  set(RAVEN_CMAKE_CMAKE_FOUND ON CACHE INTERNAL "" FORCE)
endif ()
